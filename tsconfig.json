{                                                         "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "allowJs": true,
        "checkJs": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "outDir": "./types",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "skipLibCheck": true
    },
    "files": [
        "./index.js",
        "./utils.js"
    ],
    "exclude": [
        "types"
    ]
}
